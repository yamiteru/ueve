{
  "name": "ueve",
  "type": "module",
  "version": "1.69.0",
  "license": "MIT",
  "author": "Miroslav VrsÌŒeckyÌ <yamiteru@icloud.com>",
  "description": "ğŸ”¥ Hellishly fast and ğŸ¤ tiny async/sync event emitter",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default":"./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default":"./dist/index.cjs"
      }
    },
    "./sync": {
      "import": {
        "types": "./dist/sync.d.ts",
        "default":"./dist/sync.mjs"
      },
      "require": {
        "types": "./dist/sync.d.ts",
        "default":"./dist/sync.cjs"
      }
    },
    "./async": {
      "import": {
        "types": "./dist/async.d.ts",
        "default":"./dist/async.mjs"
      },
      "require": {
        "types": "./dist/async.d.ts",
        "default":"./dist/async.cjs"
      }
    }
  },
  "files": [
    "!dist/benchmarks/",
    "!dist/tests/",
    "!dist/examples/",
    "!dist/scripts/",
    "dist/*.cjs",
    "dist/*.mjs",
    "dist/*.map",
    "dist/*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yamiteru/ueve.git",
    "directory": "src"
  },
  "keywords": [
    "event",
    "event-emitter",
    "reactive",
    "reactive-programming",
    "pub-sub",
    "pubsub",
    "javascript",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/yamiteru/ueve/issues"
  },
  "homepage": "https://github.com/yamiteru/ueve",
  "scripts": {
    "fix:format": "prettier --write \"**/*.ts\"",
    "fix:lint": "eslint --fix --ext .ts .",
    "fix": "pnpm fix:lint && pnpm fix:format",
    "check:format": "prettier --check \"**/*.ts\"",
    "check:lint": "eslint --ext .ts .",
    "check": "pnpm check:lint && pnpm check:format",
    "test:coverage": "vitest run --coverage",
    "test": "vitest run",
    "build": "pnpm clean && node --loader ts-node/esm scripts/build.ts && tsc --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm test && pnpm build",
    "release": "release-it",
    "benchmark": "node --loader ts-node/esm benchmarks/index.ts"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.51",
    "@swc/helpers": "0.5.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "@vitest/coverage-c8": "0.30.1",
    "auto-changelog": "2.4.0",
    "esbuild": "0.17.17",
    "esbuild-plugin-swc": "1.0.1",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "npm-dts": "1.3.12",
    "prettier": "2.8.7",
    "release-it": "15.10.1",
    "ts-node": "10.9.1",
    "typescript": "next",
    "vitest": "0.30.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
